{
  "name": "LRU new App",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "LRU new Tab manager",
  "background":
  {
    "service_worker":"./background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./main.js"],
      "css": ["/main.css"],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_popup": "./popup.html"
  },
  "permissions": ["storage","tabs","activeTab","contextMenus"],
  "host_permissions":["<all_urls>"]
}